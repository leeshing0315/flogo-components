{
  "name": "tcpreceiver",
  "version": "0.0.1",
  "type": "flogo:trigger",
  "ref": "github.com/leeshing0315/flogo-components/trigger/tcpreceiver",
  "description": "receive SMU TCP data",
  "author": "Max Li <leeshing0315@126.com>",
  "title": "TCP Receiver",
  "settings":[
    {
      "name": "port",
      "type": "string",
      "value": "8033",
      "required": "true"
    }
  ],
  "output": [
    {
      "name": "eventTime",
      "type": "string"
    },
    {
      "name": "ip",
      "type": "string"
    },
    {
      "name": "command",
      "type": "integer"
    },
    {
      "name": "seqNo",
      "type": "integer"
    },
    {
      "name": "reqDataSegment",
      "type": "array"
    },
    {
      "name": "cntrNum",
      "type": "string"
    },
    {
      "name": "devId",
      "type": "string"
    },
    {
      "name": "pin",
      "type": "string"
    },
    {
      "name": "firmwareVersion",
      "type": "string"
    },
    {
      "name": "devtype",
      "type": "string"
    },
    {
      "name": "emptyByteArray",
      "type": "array"
    }
  ],
  "reply": [
    {
      "name": "resDataSegment",
      "type": "array"
    },
    {
      "name": "setCommandSegment",
      "type": "array"
    },
    {
      "name": "readCommandSegment",
      "type": "array"
    },
    {
      "name": "setCommandSeqNo",
      "type": "string"
    },
    {
      "name": "readCommandSeqNo",
      "type": "string"
    },
    {
      "name": "cntrNum",
      "type": "string"
    },
    {
      "name": "devId",
      "type": "string"
    },
    {
      "name": "pin",
      "type": "string"
    },
    {
      "name": "firmwareVersion",
      "type": "string"
    },
    {
      "name": "devtype",
      "type": "string"
    }
  ],
  "handler": {
    "settings": [
      {
        "name": "handler_setting",
        "type": "string"
      }
    ]
  }
}